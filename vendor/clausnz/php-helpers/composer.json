{
  "name": "clausnz/php-helpers",
  "description": "A Collection of useful php helper functions.",
  "keywords": [
    "php",
    "helper",
    "function",
    "array",
    "device",
    "string",
    "utilities",
    "yaml"
  ],
  "type": "library",
  "version": "0.2.1",
  "require": {
    "php": ">=5.6",
    "mobiledetect/mobiledetectlib": "^2.8",
    "jaybizzle/crawler-detect": "^1.2",
    "mustangostang/spyc": "^0.6"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Claus Bayer",
      "email": "claus.bayer@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "CNZ\\Helpers\\": "src/Helpers/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "cvuorinen/phpdoc-markdown-public": "^0.2",
    "mikey179/vfsStream": "^1.5.0"
  },
  "config": {
    "platform": {
      "php": "5.6.0"
    },
    "optimize-autoloader": true
  }
}
